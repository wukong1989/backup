imap <C-e> <Esc>
imap <C-c> <Esc>y
noremap <C-z> u
nnoremap h i
nnoremap j h
nnoremap k j
nnoremap i k
nmap } w
nmap { b
nmap <C-l> g$
nmap <C-j> g^
nmap <C-v> p
nmap <C-s> :w
nmap <C-c> y
vnoremap h <Esc>i
vnoremap j <Esc>h
vnoremap k <Esc>j
vnoremap i <Esc>k
vmap <Tab> >
vmap <C-e> <Esc>
vmap <S-Tab> <
nmap <bs> X
nmap <S-bs> x
vmap I <Up>
vmap K <Down>
vmap J <Left>
vmap L <Right>
vmap <C-l> g$
vmap <C-j> g^
nmap I v<Up>
nmap K v<Down>
nmap J v<Left>
nmap L v<Right>
vnoremap <C-X> "+x

set backspace=indent,eol,nostop
:set clipboard=unnamed
set nohlsearch
